{
    "name": "laravel-gallery",
    "version": "1.0.0",
    "description": "Modern Image Gallery built with Laravel and Vue.js",
    "keywords": [
        "laravel",
        "vue",
        "gallery",
        "images",
        "photography",
        "tailwind"
    ],
    "author": {
        "name": "Gallery Team",
        "email": "team@gallery.local"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/your-username/laravel-gallery.git"
    },
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "dev:host": "vite --host",
        "preview": "vite preview",
        "lint": "eslint resources/js --ext .vue,.js,.jsx,.ts,.tsx",
        "lint:fix": "eslint resources/js --ext .vue,.js,.jsx,.ts,.tsx --fix",
        "format": "prettier --write resources/js/**/*.{vue,js,jsx,ts,tsx}",
        "type-check": "vue-tsc --noEmit",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage"
    },
    "devDependencies": {
        "@headlessui/vue": "^1.7.16",
        "@heroicons/vue": "^2.0.18",
        "@inertiajs/vue3": "^1.0.14",
        "@tailwindcss/aspect-ratio": "^0.4.2",
        "@tailwindcss/forms": "^0.5.6",
        "@tailwindcss/typography": "^0.5.10",
        "@types/node": "^20.8.0",
        "@vitejs/plugin-vue": "^4.4.0",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.1",
        "autoprefixer": "^10.4.16",
        "axios": "^1.6.0",
        "eslint": "^8.52.0",
        "eslint-plugin-vue": "^9.17.0",
        "jsdom": "^22.1.0",
        "laravel-vite-plugin": "^0.8.1",
        "postcss": "^8.4.31",
        "prettier": "^3.0.3",
        "tailwindcss": "^3.3.5",
        "typescript": "^5.2.2",
        "vite": "^4.5.0",
        "vitest": "^0.34.6",
        "vue": "^3.3.6",
        "vue-tsc": "^1.8.19"
    },
    "dependencies": {
        "@floating-ui/dom": "^1.5.3",
        "@vueuse/core": "^10.5.0",
        "date-fns": "^2.30.0",
        "lodash": "^4.17.21",
        "sortablejs": "^1.15.0",
        "vue-router": "^4.2.5",
        "ziggy-js": "^2.0.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "prettier": {
        "semi": false,
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "es5",
        "printWidth": 100,
        "bracketSpacing": true,
        "arrowParens": "avoid"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended",
            "@vue/typescript/recommended",
            "@vue/prettier"
        ],
        "parserOptions": {
            "ecmaVersion": 2020
        },
        "rules": {
            "no-console": "warn",
            "no-debugger": "warn",
            "vue/multi-word-component-names": "off",
            "@typescript-eslint/no-unused-vars": "warn",
            "@typescript-eslint/no-explicit-any": "off"
        }
    },
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {}
        }
    }
}
